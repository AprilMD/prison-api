{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"title": "Location Count Detail",
	"type": "object",
	"properties": {
		"locationCountId": { "type": "integer" },
		"countType": {
			"type": "string",
			"description": "The id to the LoV for the count." },
		"countTime": { "type": "string" },
		"countStatus": {
			"type": "string",
			"enum": ["Pending Count", "Pending Recount", "Completed"] }
	},
	"required": ["locationCountId", "countType", "countTime", "countStatus"]
}
