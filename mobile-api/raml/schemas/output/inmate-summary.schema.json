{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Inmate Summary",
  "type": "object",
  "properties": {
	"bookingId": { "type": "integer" },
	"bookingNo": { "type": "string" },
	"offenderNo": { "type": "string" },
	"firstName": { "type": "string" },
	"middleName": { "type": "string" },
	"lastName": { "type": "string" },
    "dateOfBirth":  { "type": "string" },
    "age":   { "type": "integer" },
	"alertsCodes": { "type": "array", "items": { "type": "string" } },
	"agencyId": { "type": "string" },
	"assignedLivingUnitId": { "type": "integer" },
    "assignedLivingUnitDesc": { "type": "string" },
	"facialImageId": { "type": "integer" },
	"assignedOfficerUserId": { "type": "string"},
    "aliases": { "type": "array", "items": { "type": "string" } }
  },
  "required": ["bookingNo", "bookingId", "offenderNo", "firstName", "lastName", "dateOfBirth", "agencyId"]
}