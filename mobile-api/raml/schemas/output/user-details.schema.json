{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"title": "User Details",
	"type": "object",
	"properties": {
		"userId": { "type": "integer" },
		"username": { "type": "string" },
	  "email": { "type": "string" },
		"thumbnailId": { "type": "integer" },
		"roles": {
			"type": "array",
			"items": { "type": "string" }
		}
	},
	"required": ["userId", "username"]
}
