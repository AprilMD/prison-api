{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"title": "Inmate Assignment Summary",
	"type": "object",
	"properties": {
		"bookingId": { "type": "integer" },
		"bookingNo": { "type": "string" },
		"offenderNo": { "type": "string" },
		"firstName": { "type": "string" },
		"lastName": { "type": "string" },
		"dateOfBirth": {
			"type": "string",
			"format": "date-only"
		},
		"alertsCodes": { "type": "array", "items": { "type": "string" } },
		"agencyId": { "type": "string" },
		"assignedLivingUnitId": { "type": "integer" },
		"assignedLivingUnitDesc": { "type": "string" },
		"facialImageId": { "type": "integer" }
	},
	"required": ["inmateId", "bookingId", "offenderId", "firstName", "lastName", "agencyId"]
}