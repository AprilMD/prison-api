{
	"$schema": "http://json-schema.org/draft-04/schema",
	"type": "object",
	"title": "CaseNote",
	"properties": {
		"caseNoteId":   { "type": "integer" },
		"bookingId":   { "type": "integer" },
		"type":   { "type": "string" },
		"typeDescription":   { "type": "string" },
		"subType": { "type": "string" },
		"subTypeDescription": { "type": "string" },
		"source": { "type": "string" },
		"creationDateTime":   { "type": "string", "format": "datetime-only" },
		"occurrenceDateTime":  { "type": "string", "format": "datetime-only" },
		"authorUserId": { "type": "string" },
		"text": { "type": "string" }
	},
	"required": ["caseNoteId", "bookingId", "type", "typeDescription", "subType", "subTypeDescription", "text"]
}