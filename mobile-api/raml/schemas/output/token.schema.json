{
  "$schema": "http://json-schema.org/draft-04/schema",
  "type": "object",
  "title": "TokenAuth",
  "properties": {
	"token":   { "type": "string" },
    "issuedAt": { "type": "integer" },
	"expiration": { "type": "integer" },
    "refreshToken": { "type": "string" },
    "refreshExpiration": { "type": "integer" }
  },
  "required": ["token", "issuedAt", "expiration", "refreshToken", "refreshExpiration"]
}
