#%RAML 1.0 DataType
description: Access Role
properties:
  roleCode:
    type: string
    description: unique code for the access role
    example: CUSTODIAL_MGR
  roleName:
    type: string
    description: name of the access role
    example: Custodial Manager
  parentRoleCode?:
    type: string
    description: role code of the parent role
