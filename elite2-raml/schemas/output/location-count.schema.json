{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"title": "Location Count",
  "type": "object",
	"properties": {
		"physicalCountId": { "type": "integer" },
		"locationId": {"type": "integer" },
		"enteredByUserId": { "type": "integer" },
		"initialCount": { "$ref": "locationCountDetail"},
		"recount": { "$ref": "locationCountDetail"},
		"countStatusCode": { "enum": ["I", "R", "C"] }
	},
	"required": ["physicalCountId", "enteredByUserId", "initialCount"]
}
